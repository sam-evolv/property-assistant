{
  "name": "@openhouse/unified-portal",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -H 0.0.0.0 -p 5000",
    "build": "next build",
    "build:standalone": "NEXT_TELEMETRY_DISABLED=1 next build",
    "package:replit": "rm -rf .replit-deploy && mkdir -p .replit-deploy && cp -R .next/standalone/* .replit-deploy/ && mkdir -p .replit-deploy/.next && cp -R .next/static .replit-deploy/.next/static && cp -R public .replit-deploy/public || true",
    "start": "next start -p $PORT",
    "typecheck": "tsc --noEmit",
    "lint": "next lint || true",
    "smoke": "bash scripts/smoke-test.sh http://localhost:5000",
    "load-test": "node scripts/load-test.js http://localhost:5000 10 10",
    "test:stability": "npm run typecheck && npm run build && npm run smoke"
  },
  "dependencies": {
    "@babel/runtime": "^7.28.4",
    "@openhouse/api": "1.0.0",
    "@openhouse/db": "0.1.0",
    "@radix-ui/react-tabs": "^1.0.4",
    "@supabase/auth-helpers-nextjs": "^0.8.7",
    "@supabase/supabase-js": "^2.88.0",
    "abort-controller": "^3.0.0",
    "agentkeepalive": "^4.6.0",
    "autoprefixer": "^10.4.21",
    "csv-parse": "^5.5.6",
    "dom-helpers": "^5.2.1",
    "fast-equals": "^5.0.1",
    "form-data-encoder": "^1.7.2",
    "jspdf": "^2.5.2",
    "lodash": "^4.17.21",
    "lucide-react": "^0.454.0",
    "nanoid": "^5.0.7",
    "next": "^14.2.18",
    "node-domexception": "^1.0.0",
    "openai": "^4.73.0",
    "pdf-lib": "^1.17.1",
    "pdf-parse": "^2.4.5",
    "postcss": "^8",
    "prop-types": "^15.8.1",
    "qrcode": "^1.5.4",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hot-toast": "^2.4.1",
    "react-transition-group": "^4.4.5",
    "recharts": "^2.13.3",
    "recharts-scale": "^0.4.5",
    "set-cookie-parser": "^2.7.2",
    "styled-jsx": "^5.1.7",
    "swr": "^2.2.5",
    "tailwindcss": "^3.4.1",
    "tiny-invariant": "^1.3.3",
    "xlsx": "^0.18.5",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/pdf-parse": "^1.1.5",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "typescript": "^5"
  }
}
