+    },
+    {
+      id: 'status',
+      header: 'Status',
+      accessor: 'status',
+      sortable: true,
+      cell: (value) => {
+        const config = statusConfig[value as SnagStatus];
+        return (
+          <Badge
+            variant={
+              value === 'verified' || value === 'resolved'
+                ? 'success'
+                : value === 'in-progress'
+                ? 'info'
+                : value === 'acknowledged'
+                ? 'warning'
+                : 'error'
+            }
+            size="sm"
+          >
+            {config.label}
+          </Badge>
+        );
+      },
+    },
+    {
+      id: 'priority',
+      header: 'Priority',
+      accessor: 'priority',
+      sortable: true,
+      cell: (value) => {
+        const config = priorityConfig[value as SnagPriority];
+        return (
+          <span className={cn('text-sm font-medium', config.color)}>
+            {config.label}
+          </span>
+        );
+      },
+    },
+    {
+      id: 'days',
+      header: 'Days',
+      accessor: 'daysOpen',
+      sortable: true,
+      align: 'right',
+      cell: (value, row) => (
+        <span
+          className={cn(
+            'text-sm',
+            (value as number) > 5 && !['resolved', 'verified'].includes(row.status)
+              ? 'text-red-600 font-medium'
+              : 'text-gray-600'
+          )}
+        >
+          {value as number}
+        </span>
+      ),
+    },
+  ];
