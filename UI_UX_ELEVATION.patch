+  placeholder?: string;
+  onSearch?: (query: string) => Promise<CommandItem[]> | CommandItem[];
+  className?: string;
+}
+
+// Default navigation commands
+const defaultCommands: CommandItem[] = [
+  {
+    id: 'nav-overview',
+    label: 'Go to Overview',
