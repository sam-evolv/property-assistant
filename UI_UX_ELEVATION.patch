+              </button>
+            </Dialog.Close>
+          </div>
+
+          {/* Content */}
+          <div className="p-4 max-h-[60vh] overflow-y-auto">
+            {/* Recipients */}
+            <div className="mb-4">
+              <div className="flex items-center gap-2 mb-3">
+                <Users className="w-4 h-4 text-gray-400" />
+                <h4 className="text-sm font-medium text-gray-700">
+                  Recipients ({recipients.length})
