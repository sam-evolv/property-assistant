+                label: 'Assign Contractor',
+                icon: User,
+                onClick: (ids) => console.log('Assign contractor to', ids),
+              },
+              {
+                label: 'Mark Resolved',
+                icon: CheckCircle,
+                onClick: (ids) => console.log('Mark resolved', ids),
+              },
+            ]}
+            emptyMessage="No snags match your filters"
+          />
+        </div>
+      </div>
+    </div>
+  );
+}
